{
  "name": "landing",
  "private": true,
  "version": "0.1.0",
  "license": "MPL-2.0",
  "homepage": "https://opencomet.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/opencomet-io/landing"
  },
  "author": {
    "name": "Igor Michalak",
    "url": "https://github.com/igormichalak"
  },
  "packageManager": "yarn@3.4.1",
  "scripts": {
    "postinstall": "husky install && yarn run gen:msw && yarn run gen:partytown",
    "gen:msw": "msw init public/ --save",
    "gen:partytown": "partytown copylib public/~partytown",
    "gen:sitemaps": "next-sitemap && prettier --write './public/*.xml'",
    "build": "yarn run gen:partytown && NODE_OPTIONS='--no-experimental-fetch' next build && yarn run gen:sitemaps",
    "dev": "NODE_OPTIONS='--no-experimental-fetch' next dev",
    "start": "NODE_OPTIONS='--no-experimental-fetch' next start --keepAliveTimeout=22000",
    "lint": "next lint",
    "format:all": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.7.5",
    "@mantine/hooks": "^5.10.3",
    "@next/font": "13.1.6",
    "downshift": "^7.2.0",
    "framer-motion": "^9.0.2",
    "i18next": "^22.4.9",
    "immer": "^9.0.19",
    "next": "13.1.6",
    "next-i18next": "^13.1.5",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "react-i18next": "^12.1.5",
    "react-is": "^18.2.0",
    "sharp": "^0.31.3",
    "styled-components": "^5.3.6",
    "ua-parser-js": "^1.0.33",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "^5",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.4",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "msw": "^1.0.1",
    "next-sitemap": "^3.1.52",
    "prettier": "^2.8.4",
    "svgo": "^3.0.2",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "resolutions": {
    "@types/testing-library__jest-dom": "opencomet-io/jest-dom-types"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
